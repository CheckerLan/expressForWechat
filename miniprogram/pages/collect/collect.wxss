.inputview{
  background-color: #fff;
  margin: 20rpx;
  border-radius: 20rpx;
  font-size: 30rpx;
  align-items: center;
}
.inputview input{
  background-color: #fff;

  height: 80rpx;
  line-height: 80rpx;
  width: 550rpx;
  overflow: hidden;

  border-radius: 5rpx;
  font-size: 35rpx;
  
  text-overflow: ellipsis;
  white-space: nowrap;
}

.underline{
  border-bottom: 3rpx solid gainsboro;
}
.f-001{
  font-size: 25rpx;
}